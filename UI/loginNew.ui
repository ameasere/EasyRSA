/* /////////////////////////////////////////////////////////////////////////////////////////////////
Tooltip */
QToolTip {
	color: #ffffff;
	background-color: rgba(35, 36, 42, 180);
	border: 1px solid rgb(35,35,38);
	background-image: none;
	background-position: left center;
    background-repeat: no-repeat;
	border: none;
	border-left: 2px solid rgb(0,94,217);
	text-align: left;
	padding-left: 8px;
	margin: 0px;
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
Bg App */
#bgApp {
	background-color: rgb(35, 36, 42);
	border: 1px solid rgb(35,35,38);
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
Left Menu */
#leftMenuBg {
	background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:1, stop:0 rgba(97, 67, 133, 255), stop:1 rgba(81, 99, 149, 255));
}
#topLogo {
	background-color: transparent;
	background-image: url(:/images/images/images/RSA30x30.png);
	background-position: center;
	background-repeat: no-repeat;
}
#titleLeftApp { font: 81 12pt &quot;Raleway ExtraBold&quot;; }
#titleLeftDescription { font: 8pt &quot;Segoe UI&quot;; color: #aa63ff; }

/* MENUS */
#topMenu .QPushButton {
	background-position: left center;
    background-repeat: no-repeat;
	border: none;
	border-left: 22px solid transparent;
	background-color: transparent;
	text-align: left;
	padding-left: 44px;
}
#topMenu .QPushButton:hover {
	background-color: rgb(35,35,38);
}
#topMenu .QPushButton:pressed {
	background-color: #aa63ff;
	color: #fff;
}
#bottomMenu .QPushButton {
	background-position: left center;
    background-repeat: no-repeat;
	border: none;
	border-left: 20px solid transparent;
	background-color:transparent;
	text-align: left;
	padding-left: 44px;
}
#bottomMenu .QPushButton:hover {
	background-color: rgb(35,35,38);
}
#bottomMenu .QPushButton:pressed {
	background-color: #aa63ff;
	color: #fff;
}
#leftMenuFrame{
	border-top: 3px solid rgb(35,35,38);
}

/* Toggle Button */
#toggleButton {
	background-position: left center;
